<!-- Versions table-->
<div class="form-element-align">
  <table class="table table-responsive table-striped table-hover table-sm table-bordered">
    <thead>
    <tr>
      <th>Id</th>
      <th>Event</th>
      <th>Who</th>
      <th>Date</th>
      <th>Object</th>
    </tr>
    </thead>
    <tbody>
    <% versions.each do |version| %>
        <tr>
          <td><%= version.id %></td>
          <td><%= version.event %></td>
          <td><%= User.where("id=?", version.whodunnit.to_i).pluck(:email)[0] unless version.whodunnit.blank? %></td>
          <td><%= version.created_at.strftime("%d/%m/%Y %H:%M") %></td>
          <td><%= version.object %></td>
        </tr>
    <% end %>
    </tbody>
  </table>

  <%= paginate versions, views_prefix: 'aca_rails' %>
</div>